<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ztobbal.kiestcho">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_kec"
        android:label="KiEstCho"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ztobbal.kiestcho.ProfilAutreUtilisateur"></activity>
        <activity android:name="com.ztobbal.kiestcho.Conversation" />
        <activity android:name="com.ztobbal.kiestcho.Parametre_Profil" />
        <activity
            android:name="com.ztobbal.kiestcho.MonProfil"
            android:label="@string/title_activity_profil"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.ztobbal.kiestcho.Accueil_test" />
        <activity android:name="com.ztobbal.kiestcho.Accueil" />
        <activity android:name="com.ztobbal.kiestcho.Inscription" />
        <activity android:name="com.ztobbal.kiestcho.Authentification">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>